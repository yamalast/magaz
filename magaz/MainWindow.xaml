<Window x:Class="magaz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:magaz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Grid.Row="0">
            <Image Source="/Resources/Лого ТНС.jpg" />
            <TextBlock Text="Ебаная срань сука" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </StackPanel>
        <Frame x:Name="FrNav" Grid.Row="1"/>
    </Grid>
</Window>
